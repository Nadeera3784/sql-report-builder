{
  "name": "starterpack",
  "version": "1.0.0",
  "description": "expressjs starter project",
  "main": "app.js",
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "lint": "eslint \"./**/*.js\" --quiet",
    "lintFull": "eslint \"./**/*.js\"",
    "lintFix": "eslint --fix './**/*.js'"
  },
  "keywords": [
    "expressjs"
  ],
  "author": "nadeera3784",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "csv-writer": "^1.6.0",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.3",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "eyes": "^0.1.8",
    "helmet": "^4.0.0",
    "i18n-2": "^0.7.3",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.5",
    "md5": "^2.3.0",
    "moment": "^2.27.0",
    "mongoose": "^5.9.27",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.0",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.7.2",
    "nodemailer-sendgrid": "^1.0.3",
    "sequelize": "^6.9.0",
    "winston": "^3.3.3",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  }
}
